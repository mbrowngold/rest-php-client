{
  "name": "sugarcrm/rest-php-client",
  "description": "An SDK for interacting with Sugar's REST v10 API.",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Mike Russell",
      "email": "mrussell@sugarcrm.com",
      "role": "Developer"
    }
  ],
  "keywords": [
    "sugarcrm",
    "sugar",
    "crm",
    "rest",
    "sdk",
    "v10",
    "api"
  ],
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^3|^4|^5|^6|^7|^8",
    "vimeo/psalm": "^3"
  },
  "autoload": {
    "psr-4": {
      "SugarAPI\\SDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SugarAPI\\SDK\\Tests\\": "tests/"
    }
  }
}
